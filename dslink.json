{
  "name": "dslink-dart-mongodb",
  "version": "1.0.0",
  "description": "MongoDB DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.9.0"
  },
  "getDependencies": [
  ],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "MongoDB"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "token": {
      "type": "string"
    },
    "old": {
      "type": "bool",
      "default": false
    }
  }
}
